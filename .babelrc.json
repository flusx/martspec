{
  "sourceType": "unambiguous",
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": 100,
          "safari": 15,
          "firefox": 91
        }
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
    // "@babel/preset-flow"
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
		  // Add your aliases here
          "src": "./src/",
          "img": "./src/img"
        }
      }
    ]
  ]
}